{
  "name": "event-flow",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "env:pull": "turbo env:pull",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint:check": "biome lint .",
    "clean": "turbo run clean",
    "marketing:dev": "turbo run dev --filter=@ef/marketing",
    "marketing:build": "turbo run build --filter=@ef/marketing",
    "admin:dev": "turbo run dev --filter=@ef/admin",
    "admin:build": "turbo run build --filter=@ef/admin",
    "platform:dev": "turbo run dev --filter=@ef/platform",
    "platform:build": "turbo run build --filter=@ef/platform",
    "storybook:dev": "turbo run dev --filter=@sustentus/storybook",
    "storybook:build": "turbo run build --filter=@sustentus/storybook",
    "ui:build": "turbo run build --filter=@ef/ui",
    "ui:dev": "turbo run dev --filter=@ef/ui",
    "services:build": "turbo run build --filter=@ef/services",
    "services:dev": "turbo run dev --filter=@ef/services",
    "email:dev": "pnpm --filter @ef/services email:dev",
    "packages:build": "turbo run build --filter=@ef/ui --filter=@ef/services --filter=@ef/db",
    "packages:dev": "turbo run dev --filter=@ef/ui --filter=@ef/services --filter=@ef/db",
    "db:seed": "tsx supabase/scripts/seed_database.ts",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@supabase/supabase-js": "^2.84.0",
    "depcheck": "^1.4.7",
    "husky": "^9.1.7",
    "tsx": "^4.8.1",
    "turbo": "^2.6.1",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.18.0"
}
